#include <stdio.h>
int main(void) {
    int course_id = 2271;
    int student_id = 20676;
    int difference = student_id - course_id;

    printf("Name: \t\t\t\tKiernan Lauderdale\n");
    printf("Course ID: \t\t\t%d\n", course_id);
    printf("Student ID: \t\t\t%d\n", student_id);
    printf("Student ID - Course ID is: \t%d\n", difference);

    return (0);
    
}